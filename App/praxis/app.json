{
  "expo": {
    "name": "praxis",
    "slug": "praxis",
    "version": "1.0.0",
    "scheme": "praxis",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "android": {
      "package": "com.anonymous.praxis"
    },
    "ios": {
      "bundleIdentifier": "com.anonymous.praxis"
    }
  }
}
